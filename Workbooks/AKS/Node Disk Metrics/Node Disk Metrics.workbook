      "content": {
        "version": "KqlItem/1.0",
        "query": "InsightsMetrics\r\n| where Origin =~ 'container.azm.ms/telegraf'            \r\n| extend Tags = todynamic(Tags)         \r\n| where Namespace =~ 'disk'            \r\n| project TimeGenerated, Computer = tostring(Tags.hostName), Device = tostring(Tags.device), Path = tostring(Tags.path), DiskMetricName = Name, DiskMetricValue = Val\r\n| where Computer =~ \"{Computer}\"\r\n| where Device =~ \"{Device}\"\r\n| where Path =~ \"{Path}\"\r\n| where (DiskMetricName == 'used') or (DiskMetricName == 'free') \r\n| summarize arg_max(TimeGenerated, *) by Computer, Device, Path, DiskMetricName\r\n| project DiskMetricName, DiskMetricValue\r\n| render piechart\r\n\r\n",
