    {
      "type": 1,
      "content": {
        "json": "<h2 style=\"font-weight:normal;\">List of Open Ports for all Computers in the Workspace</h2>"
      },
      "conditionalVisibility": null,
      "customWidth": "50"
    },
    {
      "type": 1,
      "content": {
        "json": "<h2 style=\"font-weight:normal;\">Ports in use by Processes on the selected Computer</h2>"
      },
      "conditionalVisibility": null,
      "customWidth": "50"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "// {DefaultWorkspace} {ContextSelection} {Subscriptions} {Workspaces} {TimeRange}\nVMBoundPort\n| where TimeGenerated {TimeRange}\n| where Ip != \"127.0.0.1\"\n| summarize by Computer, Port, Protocol\n| summarize OpenPorts=count() by Computer\n| project Computer, [\"Count of Open Ports\"] = OpenPorts\n| order by [\"Count of Open Ports\"] desc",
        "showQuery": false,
        "size": 1,
        "aggregation": 0,
        "showAnnotations": false,
        "exportFieldName": "Computer",
        "exportParameterName": "ComputerName",
        "showAnalytics": false,
        "timeContextFromParameter": null,
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces",
        "crossComponentResources": [
          "{Workspaces}"
        ],
        "visualization": "table",
        "gridSettings": {
          "formatters": [
            {
              "columnMatch": "Computer",
              "formatter": 1,
              "formatOptions": {
                "showIcon": true
              }
            },
            {
              "columnMatch": "Open Ports",
              "formatter": 4,
              "formatOptions": {
                "palette": "blueDark",
                "showIcon": true
              }
            }
          ],
          "rowLimit": 1000,
          "filter": true
        }
      },
      "conditionalVisibility": null,
