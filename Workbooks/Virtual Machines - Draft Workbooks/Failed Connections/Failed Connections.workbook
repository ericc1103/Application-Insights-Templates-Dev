        "version": "KqlItem/1.0",
        "query": "let overallData = VMConnection\n| where TimeGenerated {TimeRange}\n| summarize Established = sum(LinksEstablished), Failed = sum(LinksFailed)\n| extend PercentFailed = (todouble(Failed)/todouble(Failed+Established))*100.0\n| extend Computer = 'ðŸ”µ Overall', Type = '1';\nVMConnection\n| where TimeGenerated {TimeRange}\n| summarize Established = sum(LinksEstablished), Failed = sum(LinksFailed) by Computer\n| extend PercentFailed = (todouble(Failed)/todouble(Failed+Established))*100.0, Type = '2'\n| union overallData\n| order by Type asc, PercentFailed desc\n| project-away Type",
